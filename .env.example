# Local Development Environment Variables
# Copy to .env.local for local development

# Database
POSTGRES_USER=postgres
POSTGRES_PASSWORD=password
POSTGRES_DB=mycms
DATABASE_URL=postgres://postgres:password@db:5432/mycms

# n8n Configuration
N8N_DB_POSTGRESDB_HOST=db
N8N_DB_POSTGRESDB_PORT=5432
N8N_DB_POSTGRESDB_DATABASE=n8n
N8N_DB_POSTGRESDB_USER=postgres
N8N_DB_POSTGRESDB_PASSWORD=password
N8N_BASIC_AUTH_USER=admin
N8N_BASIC_AUTH_PASSWORD=password

# Application
NODE_ENV=development
NEXT_PUBLIC_CMS_URL=http://cms:4000
NEXT_PUBLIC_N8N_URL=http://n8n:5678

# AWS (only used in production)
# AWS_REGION=eu-central-1
# AWS_PARAMETER_STORE_PREFIX=/sliot-project/